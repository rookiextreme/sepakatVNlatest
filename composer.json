{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bacon/bacon-qr-code": "^2.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "geekcom/phpjasper": "^3.1",
        "guzzlehttp/guzzle": "^7.3",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.1",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "livewire-ui/modal": "^0.1.5",
        "livewire/livewire": "^2.4",
        "maatwebsite/excel": "^3.1",
        "mailjet/laravel-mailjet": "^3.0",
        "mailjet/mailjet-apiv3-php": "^1.5",
        "mews/captcha": "^3.2",
        "orangehill/iseed": "^3.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^4.0",
        "staudenmeir/laravel-migration-views": "^1.0",
        "wildbit/swiftmailer-postmark": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "hisman/laravel-make-class": "^1.0",
        "khadegd/laravel-breeze-bootstrap": "dev-main",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "oscarafdev/migrations-generator": "^2.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
